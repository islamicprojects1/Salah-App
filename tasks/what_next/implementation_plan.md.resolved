class PrayerTimeRange {
  DateTime adhanTime;      // ููุช ุงูุฃุฐุงู
  DateTime nextPrayerTime; // ุงูุตูุงุฉ ุงููุงุฏูุฉ
  
  // ุชูุณูู ุงูููุช ุจูู ุงูุตูุงุชูู:
  // 0-15%   = Very Early (ุฃูู ุงูููุช)
  // 15-40%  = Early
  // 40-70%  = On Time (ููุชุตู ุงูููุช)
  // 70-90%  = Late
  // 90-100% = Very Late (ุขุฎุฑ ุงูููุช)
  // 100%+   = Missed
  
  PrayerTimingQuality calculateQuality(DateTime prayedAt) {
    final totalMinutes = nextPrayerTime.difference(adhanTime).inMinutes;
    final elapsedMinutes = prayedAt.difference(adhanTime).inMinutes;
    final percentage = (elapsedMinutes / totalMinutes) * 100;
    
    if (percentage < 0) return PrayerTimingQuality.notYet;
    if (percentage <= 15) return PrayerTimingQuality.veryEarly;
    if (percentage <= 40) return PrayerTimingQuality.early;
    if (percentage <= 70) return PrayerTimingQuality.onTime;
    if (percentage <= 90) return PrayerTimingQuality.late;
    if (percentage <= 100) return PrayerTimingQuality.veryLate;
    return PrayerTimingQuality.missed;
  }
}
